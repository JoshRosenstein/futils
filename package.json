{
  "name": "@roseys/futils",
  "version": "0.0.17",
  "description": "My JavaScript utils library",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/futils.min.js",
  "jsdelivr": "dist/futils.min.js",
  "files": [
    "dist",
    "lib",
    "es",
    "README.md"
  ],
  "scripts": {
    "precommit": "node scripts/create-export.js && rollup -c && jest --onlyChanged --runInBand",
    "prepack": "yarn run build",
    "postpack": "npm run clean:scripts",
    "build": "node scripts/create-export.js && yarn run clean && yarn run build:es && yarn run build:cjs && yarn run build:umd",
    "build:es": "cross-env BABEL_ENV=es babel src --out-dir es --ignore *.spec.js,*.test.js ",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --out-dir lib --ignore *.spec.js,*.test.js ",
    "build:umd": "cross-env NODE_ENV=development rollup -c rollup.simple.js",
    "scripts": "node scripts/create-export.js",
    "clean": "rimraf es/* lib/* dist/* coverage/* ",
    "clean:scripts": "node scripts/clean.js",
    "clean:modules": "rimraf node_modules/*",
    "test": "jest --coverage --no-cache --ci --runInBand",
    "test:wcache": "jest --coverage --ci --runInBand",
    "test:quick": "jest --onlyChanged --runInBand",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "reinstall": "yarn run clean:modules && yarn",
    "lint": "eslint src/*.js --fix"
  },
  "keywords": [
    "ramda",
    "functional",
    "js"
  ],
  "author": "Josh Rosenstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoshRosenstein/futils/issues"
  },
  "homepage": "https://github.com/JoshRosenstein/futils#README",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-annotate-pure-calls": "^0.3.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bundlesize": "^0.17.0",
    "codecov": "^3.0.0",
    "coveralls": "3.0.1",
    "cross-env": "^5.1.6",
    "eslint-config-react-tools": "^1.1.6",
    "globby": "8.0.1",
    "jest": "23.0.0",
    "regenerator-runtime": "^0.11.1",
    "rollup": "0.59.2",
    "rollup-plugin-babel": "3.0.4",
    "rollup-plugin-buble": "^0.19.2",
    "rollup-plugin-cleanup": "3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "4.0.0"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.js?(x)",
      "**/?(*.)+(spec).js?(x)"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.css.js",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/build/**",
      "!**/dist/**",
      "!**/coverage/**",
      "!**/scripts/**",
      "!**/lib/**"
    ],
    "coveragePathIgnorePatterns": [
      "./rollup.split.js"
    ]
  },
  "bundlesize": [
    {
      "path": "dist/futils.min.js"
    }
  ],
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  }
}

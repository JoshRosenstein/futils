{
  "name": "@roseys/futils",
  "version": "2.0.1",
  "modulename": "futils",
  "description": "My JavaScript utils library",
  "main": "dist/commonjs/index.js",
  "module": "dist/es/index.js",
  "jsnext:main": "dist/es/index.js",
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "pika:build": "pika-pack build",
    "pika:publish": "pika-pack publish",
    "lint:lib": "prettier lib/** --write ",
    "template:types": "node ./scripts/createTypes.js",
    "rollup": "cross-env BABEL_ENV=es rollup -c config/rollup.simple.js",
    "commit": "git-cz",
    "copy:types": "node ./scripts/transferTypes.js",
    "copy:dist": "node ./scripts/copy-dist-typedefs.js",
    "flow:gen": "rimraf genFlowDefs/* && flow gen-flow-files src --out-dir genFlowDefs",
    "travis": "yarn run test:coverage && yarn run build",
    "build:modules": "node scripts/create-export.js && yarn run clean && yarn build:es && yarn bili",
    "build": "yarn clean && yarn build:modules",
    "build:types": "tsc --emitDeclarationOnly --outDir dist/commonjs",
    "bili": "bili --config bili.config.ts && yarn build:types",
    "build:es2": "cross-env BABEL_ENV=es babel src --out-dir es --ignore **/*.spec.ts,**/*.test.ts",
    "build:es": "tsc --declaration true --module esnext --outDir ./dist/es",
    "build:cjs": "tsc --declaration true --declarationMap true --module commonjs --outDir ./dist/lib",
    "build:bundles": "cross-env NODE_ENV=development rollup -c config/rollup.simple.js",
    "scripts": "node scripts/create-export.js",
    "clean": "rimraf es/* lib/* dist/**/*.js coverage/* ",
    "clean:modules": "rimraf node_modules/*",
    "test": "cross-env BABEL_ENV=test jest --coverage --no-cache --ci --runInBand",
    "test:wcache": "jest --coverage --ci --runInBand",
    "test:quick": "jest --onlyChanged --runInBand --bail",
    "test:clear": "jest --clearCache",
    "test:watch": "jest --watchAll --runInBand",
    "test:coverage": "jest --coverage",
    "reinstall": "yarn run clean:modules && yarn",
    "lint": "eslint src/*.js --fix",
    "flow": "flow",
    "flow:start": "flow start",
    "flow:stop": "flow stop",
    "flow:status": "flow status",
    "flow:coverage": "flow coverage",
    "flowType": "flow-typed install",
    "typedoc": "typedoc --options typedoc.ts",
    "gendoc": "cd scripts && ts-node generate",
    "update-docs": "yarn run typedoc && yarn run gendoc"
  },
  "keywords": [
    "ramda",
    "functional",
    "js"
  ],
  "author": "Josh Rosenstein",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/JoshRosenstein/futils/issues"
  },
  "homepage": "https://github.com/JoshRosenstein/futils#README",
  "dependencies": {
    "typed-is": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.2",
    "@babel/plugin-external-helpers": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-arrow-functions": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@metahub/cz-conventional-commit": "^3.0.2",
    "@pika/pack": "^0.3.5",
    "@pika/plugin-build-node": "^0.3.14",
    "@pika/plugin-build-types": "^0.3.14",
    "@pika/plugin-build-web": "^0.3.14",
    "@pika/plugin-standard-pkg": "^0.3.14",
    "@pika/plugin-ts-standard-pkg": "^0.3.14",
    "@roseys/art": "^1.0.1",
    "@types/jest": "^24.0.11",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.4.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-module-imports": "^1.0.0",
    "bili": "^4.5.2",
    "bundlesize": "^0.17.0",
    "codecov": "^3.0.0",
    "commander": "^2.19.0",
    "commitizen": "^3.0.4",
    "coveralls": "3.0.1",
    "cross-env": "^5.1.6",
    "cz-cli": "^1.0.0",
    "cz-conventional-kawaii": "^1.0.1",
    "debug": "^4.1.0",
    "deprecate": "^1.1.0",
    "dtslint": "^0.3.0",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-react-tools": "^1.1.6",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-flowtype": "^3.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsdoc": "^3.8.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-typescript": "^0.13.0",
    "flow-bin": "^0.83.0",
    "flow-copy-source": "^2.0.2",
    "flow-typed": "^2.5.1",
    "fs-extra": "^7.0.1",
    "globby": "8.0.1",
    "jest": "^24.4.0",
    "jest-cli": "^24.4.0",
    "marked": "^0.6.1",
    "microbundle": "^0.6.0",
    "prettier": "^1.16.4",
    "prettylint": "^1.0.0",
    "regenerator-runtime": "^0.11.1",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-cleanup": "^3.0.0-beta.1",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-terser": "^1.0.1",
    "rollup-plugin-typescript2": "^0.20.1",
    "ts-jest": "^24.0.0",
    "typedoc": "^0.14.2",
    "typescript": "^3.3.3333",
    "typescript-eslint-parser": "^20.1.1",
    "typescript-test-utils": "^1.2.1"
  },
  "bundlesize": [
    {
      "path": "dist/futils.min.js"
    }
  ],
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add --all"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-kawaii"
    }
  },
  "prettier": {
    "bracketSpacing": false,
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-ts-standard-pkg",
        {
          "exclude": [
            "/**/*.spec.ts"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ]
    ]
  }
}

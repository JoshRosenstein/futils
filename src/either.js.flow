// @flow
import type {Variadic} from './_types/flow/$types'

export type Either_ = <F: Variadic<boolean>>(fn1: F, fn2: F)=> either_11<F>

export type Either = ( <F : Variadic<boolean>>(fn1: F)=> either_10<F>) & Either_

declare var either_: Either_
declare var either: Either

export {either_, either}
declare export default Either




type either_10<F : Variadic<boolean>> = (fn2: F)=> either_11<F>;

type either_11<F:Variadic<boolean>> = F;

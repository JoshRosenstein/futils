type PropertyKey = string | number

type PredicateFunctionTypeWithKeyT<T> = (a: T, b: PropertyKey) => boolean

export type Any_ = <T>(
  predicateFn: PredicateFunctionTypeWithKeyT<T>,
  functor: any,
) => boolean

export type Any = (<T>(
  predicateFn: (value: T, key?: PropertyKey) => boolean,
) => (functor: any) => boolean) &
  Any_
